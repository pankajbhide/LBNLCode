            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

// Add query to sqlText variable.
sqlText = "select lbl_auth_release.location, lbl_auth_release.personid, (select displayname from person where personid = lbl_auth_release.personid) name  "
+ " from lbl_auth_release "
+ " where siteid = '" + params["paramSiteid"].replace("=" , "") + "' "
+ " and Orgid = '" +  params["paramOrgid"].replace("=" , "") + "' "
+ " and building_number like '" + params["paramBldg"].replace("=" , "") + "' "
+ " union "
+ " select lbl_auth_release.location, lbl_auth_release.personid, (select displayname from person where personid = lbl_auth_release.personid) name  from lbl_auth_release "
+ " where siteid = '" + params["paramSiteid"].replace("=" , "")  + "' "
+ " and Orgid = '" +  params["paramOrgid"].replace("=" , "") + "' "
+ " and personid = '" + params["paramPerson"].replace("=" , "") + "' "
+ " union "
+ " select lbl_auth_release.location, lbl_auth_release.personid, (select displayname from person where personid = lbl_auth_release.personid) name  from lbl_auth_release "
+ " where siteid = '" + params["paramSiteid"].replace("=" , "") + "' "
+ " and Orgid = '" +  params["paramOrgid"].replace("=" , "") + "' "
+ " and location like '" + params["paramBldg"].replace("=" , "") + "' "

// Include the Maximo where clause
//+ " where " + params["where"]
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["location"] = maximoDataSet.getString("location");	
row["personid"] = maximoDataSet.getString("personid");	
row["name"] = maximoDataSet.getString("name");


//row["datefield"] = maximoDataSet.getTimestamp("datefield");	


return (true);

]]></method>
