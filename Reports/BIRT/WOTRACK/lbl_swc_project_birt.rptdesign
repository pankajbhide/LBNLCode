            <method name="open"><![CDATA[subDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
subDataSet.open();

var sqlText = new String();

// in the params where clause the workorderid will be given by maximo
sqlText = "select * from (select  siteid, wonum, description, leadcraft, workorder.status, glaccount, "
+ " schedstart, schedfinish, person.displayname supervisor, parent, haschildren, level-1 AS levelx, "
+ " WORKORDER_GET_ROLLUP_ACTLABHRS(workorder.siteid, workorder.wonum) as rollup_actlabhrs "
+ " from workorder "
+ " left outer join person on (workorder.supervisor = person.personid) "
+ " start with workorder.siteid = ? and workorder.wonum = ? "
+ " connect by prior workorder.wonum = workorder.parent and prior workorder.siteid = workorder.siteid ) "
+ " where levelx > 0 "  // Do not report the same parent work order in the child list
;

subDataSet.setQuery(sqlText);
subDataSet.setQueryParameterValue(1, inputParams["siteid"]);
subDataSet.setQueryParameterValue(2, inputParams["wonum"]);]]></method>
