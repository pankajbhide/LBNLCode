            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

myTxn = MXReportTxnProvider.create(this.getDataSource().getName()); 
myStmt = myTxn.createStatement(); 
myStmt.setQuery(
"create or replace function WORKORDER_GET_ROLLUP_ACTLABHRS(" 
  + "P_SITEID IN maximo.workorder.siteid%TYPE, "
  + "P_WONUM IN maximo.workorder.wonum%TYPE "
  + ") "
  + "RETURN NUMBER "
  + "IS "
  + "V_TOTAL NUMBER; "
  + "BEGIN "
  + " V_TOTAL := 0; "
  
  + " select ( nvl(sum(labtrans.regularhrs + labtrans.premiumpayhours),0) ) "
  + " into V_TOTAL "
  + " from labtrans "
  + " where labtrans.refwo in "
  + " (select wonum from workorder recwo "
  + "   start with recwo.wonum = P_WONUM"
  + "   and recwo.siteid = P_SITEID "
  + "   connect by prior recwo.wonum = recwo.parent and prior recwo.siteid = recwo.siteid) "
  + " and (labtrans.lt1 = 'M' OR labtrans.lt1 = 'W'); "

  + " RETURN V_TOTAL; " 
  
  + " EXCEPTION WHEN NO_DATA_FOUND THEN "
  + " V_TOTAL := 0; "
  + " RETURN V_TOTAL; "
  + "END; " 
); 
myTxn.save(); 

var sqlText = new String();

// in the params where clause the workorderid will be given by maximo
sqlText = "select  siteid, wonum, description, leadcraft, workorder.status, glaccount, "
+ " schedstart, schedfinish, person.displayname supervisor, parent, haschildren, "
+ " WORKORDER_GET_ROLLUP_ACTLABHRS(workorder.siteid, workorder.wonum) as rollup_actlabhrs "
+ " from workorder "
+ " left outer join person on (workorder.supervisor = person.personid) "
+ " where siteid || ':' || wonum = "
+ "    (SELECT siteid || ':' || wonum FROM "
+ "      (SELECT siteid, wonum FROM workorder woup " 
+ "       START WITH woup.siteid || ':' || woup.wonum = (SELECT siteid || ':' || wonum FROM workorder WHERE " + params["where"] + " and workorder.wonum = '" + params["paramWOnum"].replace("=" , "") + "' " + " AND ROWNUM = 1) "
+ "       CONNECT BY PRIOR woup.parent = woup.wonum AND PRIOR woup.siteid = woup.siteid "
+ "       ORDER BY level DESC) "
+ "     WHERE ROWNUM = 1) "
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

row["wonum"] = maximoDataSet.getString("wonum");
row["parent"] = maximoDataSet.getString("parent");	
row["haschildren"] = maximoDataSet.getString("haschildren");
row["glaccount"] = maximoDataSet.getString("glaccount");	
row["siteid"] = maximoDataSet.getString("siteid");	
row["status"] = maximoDataSet.getString("status");	
row["description"] = maximoDataSet.getString("description");
row["leadcraft"] = maximoDataSet.getString("leadcraft")	
row["schedstart"] = maximoDataSet.getDate("schedstart");	
row["schedfinish"] = maximoDataSet.getDate("schedfinish");	
row["supervisor"] = maximoDataSet.getString("supervisor");
row["rollup_actlabhrs"] = maximoDataSet.getDuration("rollup_actlabhrs");

return (true);]]></method>
